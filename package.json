{
  "name": "rollup-ts-template",
  "version": "0.0.0",
  "description": "A rollup template for library development.",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "rollup -w -c",
    "build": "rollup -c",
    "example": "anywhere -p 8445 -s &",
    "test": "jest",
    "doc": "typedoc",
    "prepublish": "yarn version && yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tonyzhou1890/rollup-ts-template.git"
  },
  "bugs": {
    "url": "https://github.com/tonyzhou1890/rollup-ts-template/issues"
  },
  "homepage": "https://github.com/tonyzhou1890/rollup-ts-template#readme",
  "keywords": [
    "gif",
    "player",
    "canvas",
    "rollup-ts-template",
    "gif-player",
    "gif-parser"
  ],
  "author": "tonyzhou1890",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.4.0",
    "@types/jest": "^29.5.1",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "^8.23.0",
    "husky": "3.1.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.0.3",
    "prettier": "2.8.x",
    "rollup": "3.19.x",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-uglify": "^6.0.4",
    "rollup-plugin-web-worker-loader": "^1.6.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typedoc": "^0.24.7",
    "typescript": "^4.8.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{ts,js}": [
      "node --max_old_space_size=1024 ./node_modules/.bin/prettier -w",
      "node --max_old_space_size=1024 ./node_modules/.bin/eslint --fix --color",
      "git add"
    ]
  }
}
